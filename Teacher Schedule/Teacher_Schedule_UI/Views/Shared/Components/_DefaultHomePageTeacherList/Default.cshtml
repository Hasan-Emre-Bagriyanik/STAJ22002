@model List<ResultTeachersDto>

<section class="locations-1" id="locations">
    <div class="locations py-5">
        <div class="container py-lg-5 py-md-4 py-2">
            <div class="heading text-center mx-auto">
                <h3 class="title-big">Öne Çıkan Öğretmenler</h3>
            </div>
            <div class="row pt-md-5 pt-4">
                @foreach (var item in Model)
                {
                    <div class="col-lg-4 col-md-6 p-2">
                        <a href="property-single.html">
                            <div class="box16">
                                <div class="rentext-listing-category"></div>
                                <img class="img-fluid " style="height:250px;width:400px" src="@item.Image" alt="">
                                <div class="box-content">
                                    <h3 class="title">@item.FirstName @item.LastName</h3>
                                    <span class="post">@item.Branch Branşı Öğretmeni</span>
                                </div>
                            </div>
                        </a>
                    </div>
                }
                
               
            </div>
        </div>
    </div>
</section>
